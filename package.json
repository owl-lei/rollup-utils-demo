{
  "name": "rollup-utils-demo",
  "version": "1.0.0",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "index.d.ts",
  "scripts": {
    "push": "git cz",
    "build": "gulp",
    "rollup:dev": "rollup --config rollup.config.ts --configPlugin rollup-plugin-typescript2 --watch",
    "rollup:build": "rollup --config rollup.config.ts --configPlugin rollup-plugin-typescript2",
    "doc": "typedoc --options typedoc.json",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "lint:lint-staged": "lint-staged",
    "prepare": "husky install",
    "prepublishOnly": "pnpm run build"
  },
  "author": "雷厉",
  "description": "rollup打包工具包demo",
  "keywords": [
    "rollup"
  ],
  "homepage": "https://github.com/owl-lei/rollup-utils-demo/blob/main/README.md",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/plugin-transform-runtime": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/register": "^7.22.5",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@rollup/plugin-alias": "^5.0.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.2",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.4.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^0.33.0",
    "chalk": "4.0.0",
    "commitizen": "^4.3.0",
    "cross-spawn": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.3.0",
    "esbuild": "^0.18.12",
    "eslint": "^8.45.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^5.0.0",
    "fs-extra": "^11.1.1",
    "gulp": "^4.0.2",
    "gulp-conventional-changelog": "^3.0.0",
    "husky": "7.0.4",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0",
    "rollup": "^3.26.2",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-typescript2": "^0.35.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.24.8",
    "typescript": "^5.1.6",
    "vitest": "^0.33.0"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./index.d.ts"
    }
  },
  "files": [
    "dist",
    "docs",
    "coverage",
    "*.d.ts"
  ],
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
